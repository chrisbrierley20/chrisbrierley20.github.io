{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Ether: long run",
    "subtitle": "Daily ETH price in USD. Source: Investing.com",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "#FFFFFF",
    "subtitleColor": "#B3B3B3"
  },
  "config": {"background": "#404040", "view": {"stroke": "transparent"}},
  "width": 300,
  "height": 300,
  "data": {
    "name": "myData",
    "url": "https://raw.githubusercontent.com/RDeconomist/RDeconomist.github.io/main/charts/crypto/dataETH.csv"
  },
  "mark": {
    "type": "line",
    "point": false,
    "interpolate": "linear",
    "color": "hotpink"
  },
  "transform": [
    {"calculate": "year(datum.Date)", "as": "year"},
    {"filter": "datum.year>=minYear"},
    {"filter": "datum.year<=maxYear"}
  ],
  "params": [
    {
      "name": "minYear",
      "value": 2016,
      
      "bind": {
        "input": "range",
        "min": 2016,
        "max": 2021,
        "step": 1,
        "name": "Start year:"
      }
    },
    {
      "name": "maxYear",
      "value": 2021,
      "bind": {
        "input": "range",
        "min": 2016,
        "max": 2021,
        "step": 1,
        "name": "End year:"
      }
    }
  ],
  "encoding": {
    "x": {
      "field": "Date",
      "type": "temporal",
      "axis": {
        "title": null,
        "grid": false,
        "labelSeparation": 10,
        "domainColor": "#B3B3B3",
        "labelColor": "#FFFFFF",
        "tickColor": "#B3B3B3",
        "gridColor": "black"
      }
    },
    "y": {
      "field": "Price",
      "type": "quantitative",
      "title": "",
      "axis": {
        "grid": false,
        "domainColor": "#B3B3B3",
        "labelColor": "#FFFFFF",
        "tickColor": "#B3B3B3",
        "gridColor": "black",
        "format": "$,.5"
      }
    }
  }
}